ssh derk@voyager01.spaceflightnewsapi.net <<EOF
 if [ ! -d ~/snapiwebsite-test ]; then
 	 git clone https://github.com/spaceflightnewsapi/snapiwebsite -b test snapiwebsite-test
 fi
 cd ~/snapiwebsite-test
 git pull
 npm install
 npm run build
 pm2 delete snapiwebsite-test
 NODE_ENV=test pm2 serve build 3002 --name snapiwebsite-test --watch
 exit
EOF
